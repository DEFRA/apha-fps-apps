{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },  
  "AllowedHosts": "*",
  "AwsLogging": {
    "LogGroupName": ""
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "<domain_name>.com",
    "ClientId": "<client_Id>",
    "TenantId": "<tenant_Id>",
    "ClientSecret": "<client_Secret>",
    "ClientCertificates": [
    ],
    "CallbackPath": "/signin-oidc"
   },
  "LogsPath": "Logs",
  "ExceptionTypes": {
    "General": "FPS.GENERAL_EXCEPTION",
    "Database": "FPS.DB_EXCEPTION",
    "Authorization": "FPS.AUTHORIZATION_EXCEPTION"
  },
	"Serilog": {
	  "MinimumLevel": {
		"Default": "Information",
		"Override": {
		  "Microsoft": "Warning",
		  "System": "Warning",
		  "Microsoft.AspNetCore": "Warning"
		 }
	  }
	}
}
